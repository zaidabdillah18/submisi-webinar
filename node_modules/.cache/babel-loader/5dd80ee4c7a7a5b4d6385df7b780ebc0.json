AI,EAAE;AAAR,eAAxD,CAAR;AACAc,cAAAA,OAAO,CAACC,GAAR;;AA5BiD;AAAA;AA8BjDhB,cAAAA,QAAQ,CAAC,wBAAD,EAA2B,IAA3B,EAAiC;AAAEC,gBAAAA,IAAI,EAAE;AAAR,eAAjC,CAAR;AA9BiD;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgCpD,GAlEa;AAmEdiB,EAAAA,MAnEc,yBAmEe;AAAA,QAApBnB,MAAoB,SAApBA,MAAoB;AAAA,QAAZC,QAAY,SAAZA,QAAY;AAC3BD,IAAAA,MAAM,CAAC,mBAAD,CAAN;AACAA,IAAAA,MAAM,CAAC,sBAAD,EAAyB,IAAzB,EAA+B;AAAEE,MAAAA,IAAI,EAAE;AAAR,KAA/B,CAAN;AACA,QAAMQ,cAAc,GAAG;AACrBC,MAAAA,YAAY,EAAE,IADO;AAErBC,MAAAA,KAAK,EAAE;AAFc,KAAvB;AAIAX,IAAAA,QAAQ,CAAC,gCAAD,EAAmCS,cAAnC,EAAmD;AAAER,MAAAA,IAAI,EAAE;AAAR,KAAnD,CAAR;AACD;AA3Ea,CAAhB;AA8EA,eAAe;AACbkB,EAAAA,UAAU,EAAE,IADC;AAEb5B,EAAAA,KAAK,EAALA,KAFa;AAGbG,EAAAA,SAAS,EAATA,SAHa;AAIbG,EAAAA,OAAO,EAAPA;AAJa,CAAf","sourcesContent":["import { URL_API } from \"../constants\"\r\nimport { kirimData } from '../utils'\r\n\r\nfunction state() {\r\n  return {\r\n    idPengguna: null,\r\n    namaPengguna: null\r\n  }\r\n}\r\n\r\nconst mutations = {\r\n  aturDataPengguna(state, { idPengguna, namaPengguna }) {\r\n    state.idPengguna = idPengguna\r\n    state.namaPengguna = namaPengguna\r\n  },\r\n  resetDataPengguna(state) {\r\n    state.idPengguna = null\r\n    state.namaPengguna = null\r\n  }\r\n}\r\n\r\nconst actions = {\r\n  async masuk({ commit, dispatch }, { namaPengguna }) {\r\n    try {\r\n      dispatch('proses/tampilkanProses', null, { root: true })\r\n      const url = `${URL_API}/user/login`\r\n\r\n      const respon = await kirimData(url, {\r\n        name: namaPengguna\r\n      })\r\n\r\n      if (respon.success && !respon.error) {\r\n        commit('aturDataPengguna', {\r\n          idPengguna: respon.data.id,\r\n          namaPengguna: respon.data.name\r\n        })\r\n        const dataNotifikasi = {\r\n          apakahTampil: true,\r\n          pesan: 'Berhasil masuk'\r\n        }\r\n        await dispatch('notifikasi/tampilkanNotifikasi', dataNotifikasi, { root: true })\r\n      } else {\r\n        throw new Error(respon.message)\r\n      }\r\n    } catch (error) {\r\n      const dataNotifikasiGalat = {\r\n        apakahTampil: true,\r\n        pesan: error.message\r\n      }\r\n      dispatch('notifikasi/tampilkanNotifikasi', dataNotifikasiGalat, { root: true })\r\n      console.log(error)\r\n    } finally {\r\n      dispatch('proses/hilangkanProses', null, { root: true })\r\n    }\r\n  },\r\n  async daftar({ commit, dispatch }, { namaPengguna }) {\r\n    try {\r\n      dispatch('proses/tampilkanProses', null, { root: true })\r\n      const url = `${URL_API}/user/register`\r\n\r\n      const respon = await kirimData(url, {\r\n        name: namaPengguna\r\n      })\r\n\r\n      if (respon.success && !respon.error) {\r\n        commit('aturDataPengguna', {\r\n          idPengguna: respon.data.id,\r\n          namaPengguna: respon.data.name\r\n        })\r\n        const dataNotifikasi = {\r\n          apakahTampil: true,\r\n          pesan: 'Berhasil daftar'\r\n        }\r\n        await dispatch('notifikasi/tampilkanNotifikasi', dataNotifikasi, { root: true })\r\n      } else {\r\n        throw new Error(respon.message)\r\n      }\r\n    } catch (error) {\r\n      const dataNotifikasiGalat = {\r\n        apakahTampil: true,\r\n        pesan: error.message\r\n      }\r\n      dispatch('notifikasi/tampilkanNotifikasi', dataNotifikasiGalat, { root: true })\r\n      console.log(error)\r\n    } finally {\r\n      dispatch('proses/hilangkanProses', null, { root: true })\r\n    }\r\n  },\r\n  keluar({ commit, dispatch }) {\r\n    commit('resetDataPengguna')\r\n    commit('kode/resetDaftarKode', null, { root: true })\r\n    const dataNotifikasi = {\r\n      apakahTampil: true,\r\n      pesan: 'Berhasil keluar'\r\n    }\r\n    dispatch('notifikasi/tampilkanNotifikasi', dataNotifikasi, { root: true })\r\n  }\r\n}\r\n\r\nexport default {\r\n  namespaced: true,\r\n  state,\r\n  mutations,\r\n  actions\r\n}\r\n"]}]}